today = new Date

var oey=today.getFullYear()
var ney=oey-1969
var yos=ney%7

noy = new Array("Moonyear", "Atomyear", "Wateryear", "Windyear", "Fireyear", "Earthyear", "Sunyear")

moy = new Array("first month", "second month", "third month", "fourth month", "fifth month", "sixth month", "seventh month", "eighth month", "ninth month", "tenth month", "eleventh month", "twelfth month")

dow = new Array("Sunday", "Moonday", "Atomday", "Waterday", "Winday", "Fireday", "Earthday")

document.write("<a class='tec_en_1' href='http://www.hyperlinker.tk/tec/' style='text-decoration: none' target='TheEarthCalendar' title='The Earth Calendar'>Today is " + dow[today.getDay()] + " " + today.getDate() + ", " + moy[today.getMonth()] + ", " + noy[yos] + " " + ney + "</a>")


// second string computation

// get the current date
var today = new Date();

// get the year of the current date
var year = today.getFullYear();

// create a new Date object with the first day of the year
var first_day = new Date(year, 0, 1);

// calculate the number of milliseconds between the current date and the first day of the year
var delta = today - first_day;

// convert the number of milliseconds to days (86400000 milliseconds in a day)
var day_of_year = Math.floor(delta / 86400000) + 1;


pod = new Array("0", "good mood", "virtuosity", "conscience", "robustness", "decision", "imagination", "skill", "dexterity", "fidelity", "happiness", "compassion", "generosity", "altruism", "attention", "liveliness", "consistency", "excellence", "perspicacity", "self-criticism", "splendor", "delicacy", "credibility", "speed", "brilliance", "certainty", "planning", "shrewdness", "remembrance", "hospitality", "scrupulousness", "indefatigability", "mastery", "diplomacy", "intrepidity", "providence", "flexibility", "distinction", "honor ", "independence", "expertise", "agreement", "lucidity", "verification", "kindness", "gratitude", "truthfulness", "meditation", "solicitude", "concreteness", "direction", "musicality", "bliss", "cohesion", "completeness", "elegance", "temper", "cordiality", "nobility", "vigor", "harmony", "correctness", "humility", "frugality", "intuition", "reconciliation", "virtue", "precision", "purity", "wealth", "foresight", "exactness", "delight", "equanimity", "vitality", "wit" , "gaiety", "self-discipline", "enthusiasm", "protection", "timeliness", "subtlety", "synthesis", "joy", "inventiveness", " simplicity", "initiative", "forgiveness", "prudence", "discernment", "foresight", "concordance", "magnanimity", "observation", "receptivity" , "variety", "kindness", "quietness", "intelligence", "breadth", "playfulness", "mercy", "placidity", "rhythm", "zeal", "versatility", "sociability", "tolerance", "calm", "acceptance", "resolution", "interest", "incisiveness", "reflection" , "dignity", "charisma", "efficacy", "justice", "measurement", "greatness", "acumen", "quickness", "authoritativeness", "sincerity", "vastness", "balance", "utility", "concentration", "logic", "temperance", "saving", "accuracy", "diligence", "contemplation", "order", "freshness", "respect", "good will", "industry", "good natured", "harmony", "beauty", "demeanor", "inspiration", "elevation", "decency", "consideration", "discretion", "repentance", "charity", "agility", "philanthro